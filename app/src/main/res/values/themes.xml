  <style name="HeliosTheme" parent="Theme.AppCompat.DayNight.DarkActionBar">
    <item name="colorPrimary">@color/helios_primary</item>
    <item name="colorPrimaryDark">@color/helios_primary_dark</item>
    <item name="colorAccent">@color/helios_accent</item>
    <item name="helios_primary">@color/helios_primary</item>
    <item name="helios_accent">@color/helios_accent</item>
    <item name="msg_out">@color/msg_out</item>
    <item name="android:textColorLink">@color/text_link</item>
    <item name="android:windowBackground">@color/window_background</item>
    <item name="android:windowAnimationStyle">@style/ActivityAnimation</item>
    <item name="windowActionModeOverlay">true</item>
    <item name="alertDialogTheme">@style/HeliosDialogTheme.Neutral</item>
    <item name="preferenceTheme">@style/PreferenceThemeOverlay.v14.Material</item>
  </style>

  <style name="HeliosTheme.NoActionBar" parent="HeliosTheme">
    <!--<item name="windowActionBar">false</item>-->
    <item name="windowNoTitle">true</item>
    <item name="toolbarStyle">@style/HeliosToolbar</item>
  </style>

  <style name="HeliosTheme.ActionBarOverlay" parent="HeliosTheme.NoActionBar">
    <item name="android:windowActionBarOverlay">true</item>
  </style>

  <style name="ActivityAnimation" parent="@android:style/Animation.Activity">
    <item name="android:activityOpenEnterAnimation">@anim/screen_new_in</item>
    <item name="android:activityOpenExitAnimation">@anim/screen_old_out</item>
    <item name="android:activityCloseEnterAnimation">@anim/screen_old_in</item>
    <item name="android:activityCloseExitAnimation">@anim/screen_new_out</item>
  </style>

  <style name="HeliosDialogTheme" parent="Theme.AppCompat.DayNight.Dialog.MinWidth">
    <item name="colorPrimary">@color/helios_primary</item>
    <item name="helios_primary">@color/helios_primary</item>
    <item name="helios_accent">@color/helios_primary</item>
    <item name="colorPrimaryDark">@color/helios_primary_dark</item>
    <item name="colorAccent">@color/helios_accent</item>
    <item name="buttonBarPositiveButtonStyle">@style/HeliosButtonFlat.Positive</item>
    <item name="buttonBarNeutralButtonStyle">@style/HeliosButtonFlat.Neutral</item>
    <item name="buttonBarNegativeButtonStyle">@style/BasicButtonFlat.Negative</item>
    <item name="android:textColorLink">@color/text_link</item>
    <item name="android:windowBackground">@color/window_background</item>
    <item name="android:windowAnimationStyle">@style/DialogAnimation</item>
    <item name="android:filterTouchesWhenObscured">true</item>
  </style>

  <!-- Use this with care. Only used for the screen filter warning dialog -->
  <style name="HeliosDialogThemeNoFilter" parent="HeliosDialogTheme">
    <item name="android:filterTouchesWhenObscured">false</item>
  </style>

  <!-- Uses neutral colors for the buttons -->
  <style name="HeliosDialogTheme.Neutral" parent="HeliosDialogTheme">
    <item name="buttonBarPositiveButtonStyle">@style/HeliosButtonFlat.Neutral</item>
    <item name="buttonBarNeutralButtonStyle">@style/HeliosButtonFlat.Neutral</item>
    <item name="buttonBarNegativeButtonStyle">@style/HeliosButtonFlat.Neutral</item>
  </style>

  <style name="DialogAnimation" parent="@android:style/Animation.Dialog">
    <item name="android:windowEnterAnimation">@anim/fade_in</item>
    <item name="android:windowExitAnimation">@anim/fade_out</item>
  </style>

  <style name="OnboardingDialogTheme" parent="HeliosDialogTheme">
    <item name="android:background">@color/helios_primary</item>
    <item name="android:textColorPrimary">@color/text_primary_inverse</item>
    <item name="android:textColorSecondary">@color/text_secondary_inverse</item>
    <item name="buttonBarNeutralButtonStyle">@style/Widget.AppCompat.Button.Borderless</item>
  </style>

  <style name="TranslucentTheme" parent="Theme.AppCompat.NoActionBar">
    <item name="android:colorBackgroundCacheHint">@null</item>
    <item name="android:windowAnimationStyle">@android:style/Animation</item>
    <item name="android:windowBackground">@android:color/transparent</item>
    <item name="android:windowContentOverlay">@null</item>
    <item name="android:windowIsTranslucent">true</item>
    <item name="android:windowNoTitle">true</item>
  </style>

</resources>
